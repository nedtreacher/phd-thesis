issn = {2079-4991},
journal = {Nanomaterials 2021, Vol. 11, Page 226},
keywords = {CVD graphene,aptamer,free biosensing,label,mycotoxins,phase interferometry,spectral},
month = {jan},
number = {1},
pages = {226},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Spectral-Phase Interferometry Detection of Ochratoxin A via Aptamer-Functionalized Graphene Coated Glass}},
url = {https://www.mdpi.com/2079-4991/11/1/226/htm https://www.mdpi.com/2079-4991/11/1/226},
volume = {11},
