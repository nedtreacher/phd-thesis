file = {:C\:/Users/perkinne/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Garc{\'{i}}a-Aljaro et al. - 2010 - Carbon nanotubes-based chemiresistive biosensors for detection of microorganisms.pdf:pdf},
issn = {09565663},
journal = {Biosensors and Bioelectronics},
keywords = {Bacteriophage,Biosensor,Carbon nanotube,Escherichia coli,Pathogen,Virus},
month = {dec},
number = {4},
pages = {1437--1441},
pmid = {20729063},
title = {{Carbon nanotubes-based chemiresistive biosensors for detection of microorganisms}},
volume = {26},
year = {2010}
