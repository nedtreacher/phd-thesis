pages = {477},
publisher = {MDPI},
title = {{Measuring the Diameter of Single-Wall Carbon Nanotubes Using AFM}},
url = {https://www.mdpi.com/2079-4991/13/3/477/htm https://www.mdpi.com/2079-4991/13/3/477},
volume = {13},
year = {2023}
