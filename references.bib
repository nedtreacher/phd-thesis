pages = {045108},
publisher = {IOP Publishing},
title = {{A two-stage method for real-time baseline drift compensation in gas sensors}},
url = {https://iopscience.iop.org/article/10.1088/1361-6501/ac491f https://iopscience.iop.org/article/10.1088/1361-6501/ac491f/meta},
