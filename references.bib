issn = {10916490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Bioelectronics,DNA strand displacement,Electrical biosensor,Graphene fet DNA sensor,Snp detection},
month = {6},
number = {26},
pages = {7088--7093},
pmid = {27298347},
publisher = {National Academy of Sciences},
title = {{Highly specific SNP detection using 2D graphene electronics and DNA strand displacement}},
url = {https://www.pnas.org/doi/abs/10.1073/pnas.1603753113},
volume = {113},
