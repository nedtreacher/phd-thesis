keywords = {Biosensor,Carbon nanotube,Field effect transistor,Prostate cancer,Spacer,Surface modification},
month = {7},
number = {11},
pages = {3372--3378},
pmid = {19481922},
publisher = {Biosens Bioelectron},
title = {{Enhancement of sensitivity and specificity by surface modification of carbon nanotubes in diagnosis of prostate cancer based on carbon nanotube field effect transistors}},
