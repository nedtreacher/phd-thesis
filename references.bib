journal = {Langmuir : the ACS journal of surfaces and colloids},
keywords = {Aptamers,Biosensing Techniques*,Cocaine / chemistry,DNA / chemistry,Electrochemistry / methods*,Electrodes,Equipment Design,Extramural,Humans,Kevin W Plaxco,MEDLINE,N.I.H.,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Noelle Phares,Non-P.H.S.,Nucleic Acid Conformation,Nucleotide / chemistry,Oligonucleotides / chemistry,Oxidation-Reduction,PMC2674396,PubMed Abstract,RNA / chemistry,Research Support,Ryan J White,Surface Properties,Thrombin / chemistry,U.S. Gov't,doi:10.1021/la800801v,pmid:18690727},
