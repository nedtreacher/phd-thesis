title = {{Applications of phospholipid bilayer nanodiscs in the study of membranes and membrane proteins}},
url = {https://pubs.acs.org/doi/full/10.1021/bi602371n},
volume = {46},
year = {2007}
}
@article{Bayburt2010,
abstract = {Nanodiscs are soluble nanoscale phospholipid bilayers which can self-assemble integral membrane proteins for biophysical, enzymatic or structural investigations. This means for rendering membrane proteins soluble at the single molecule level offers advantages over liposomes or detergent micelles in terms of size, stability, ability to add genetically modifiable features to the Nanodisc structure and ready access to both sides of the phospholipid bilayer domain. Thus the Nanodisc system provides a novel platform for understanding membrane protein function. We provide an overview of the Nanodisc approach and document through several examples many of the applications to the study of the structure and function of integral membrane proteins. {\textcopyright} 2009 Federation of European Biochemical Societies.},
