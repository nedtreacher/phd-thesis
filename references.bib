number = {1},
pages = {91--94},
publisher = {John Wiley \& Sons, Ltd},
title = {{Single-Carbon-Atomic-Resolution Detection of Odorant Molecules using a Human Olfactory Receptor-based Bioelectronic Nose}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1002/adma.200801435 https://onlinelibrary.wiley.com/doi/abs/10.1002/adma.200801435 https://onlinelibrary.wiley.com/doi/10.1002/adma.200801435},
