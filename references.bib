issn = {20794991},
journal = {Nanomaterials},
keywords = {Aluminium oxide,Biosensors,Graphene,Molecular vapour deposition,Passivation},
month = {8},
number = {8},
pmid = {34443952},
publisher = {Multidisciplinary Digital Publishing Institute  (MDPI)},
title = {{Application of Molecular Vapour Deposited Al2O3 for Graphene-Based Biosensor Passivation and Improvements in Graphene Device Homogeneity}},
url = {/pmc/articles/PMC8398646/ /pmc/articles/PMC8398646/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC8398646/},
volume = {11},
year = {2021}
}
