doi = {10.1021/nl9034219},
file = {:C\:/Users/perkinne/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Gao, Zheng, Lieber - 2010 - Subthreshold Regime has the Optimal Sensitivity for Nanowire FET Biosensors.pdf:pdf},
