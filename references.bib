issn = {0022-3727},
journal = {Journal of Physics D: Applied Physics},
month = {7},
number = {31},
pages = {313001},
publisher = {IOP Publishing},
title = {{Graphene field-effect transistors}},
url = {https://iopscience.iop.org/article/10.1088/0022-3727/44/31/313001 https://iopscience.iop.org/article/10.1088/0022-3727/44/31/313001/meta},
volume = {44},
