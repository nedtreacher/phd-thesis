title = {{Electronically monitoring biological interactions with carbon nanotube field-effect transistors}},
url = {https://pubs.rsc.org/en/content/articlehtml/2008/cs/b709567h https://pubs.rsc.org/en/content/articlelanding/2008/cs/b709567h},
