publisher = {IOP Publishing},
title = {{Oxygen-induced p-type doping of a long individual single-walled carbon nanotube}},
url = {https://iopscience.iop.org/article/10.1088/0957-4484/16/8/008 https://iopscience.iop.org/article/10.1088/0957-4484/16/8/008/meta},
