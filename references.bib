issn = {1613-6829},
journal = {Small},
keywords = {carbon nanotubes,film transistors,flexible devices,graphene,thin},
month = {4},
number = {8},
pages = {1188--1205},
publisher = {John Wiley \& Sons, Ltd},
title = {{A Review of Carbon Nanotube- and Graphene-Based Flexible Thin-Film Transistors}},
url = {https://onlinelibrary.wiley.com/doi/full/10.1002/smll.201203154 https://onlinelibrary.wiley.com/doi/abs/10.1002/smll.201203154 https://onlinelibrary.wiley.com/doi/10.1002/smll.201203154},
