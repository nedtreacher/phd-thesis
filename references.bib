issn = {1873-4235},
journal = {Biosensors & bioelectronics},
keywords = {Biomarkers,Biosensing Techniques / instrumentation*,Blood Chemical Analysis / instrumentation*,Byung Yang Lee,Carbon / chemistry*,Cultured,Electrodes,Electronic,Equipment Design,Equipment Failure Analysis,Evaluation Study,Humans,Jun Pyo Kim,MEDLINE,Male,NCBI,NIH,NLM,Nanotubes,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,Prostate-Specific Antigen / blood*,Prostatic Neoplasms / blood*,Prostatic Neoplasms / diagnosis*,PubMed Abstract,Reproducibility of Results,Research Support,Sang Jun Sim,Sensitivity and Specificity,Transistors,Tumor / blood*,Tumor Cells,doi:10.1016/j.bios.2009.04.048,pmid:19481922},
