doi = {10.1016/J.DIAMOND.2018.03.011},
file = {:C\:/Users/perkinne/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Bargaoui et al. - 2018 - Gate bias stress effect in single-walled carbon nanotubes field-effect-transistors.pdf:pdf},
issn = {0925-9635},
journal = {Diamond and Related Materials},
keywords = {Nano sensors,Single carbon nanotube field effect transistor,Stress effect,Trap states},
month = {4},
pages = {62--65},
publisher = {Elsevier},
title = {{Gate bias stress effect in single-walled carbon nanotubes field-effect-transistors}},
volume = {84},
year = {2018}
}
