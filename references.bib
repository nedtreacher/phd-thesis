title = {{The holy grail of pyrene-based surface ligands on the sensitivity of graphene-based field effect transistors}},
url = {https://pubs.rsc.org/en/content/articlehtml/2022/sd/d1sd00036e https://pubs.rsc.org/en/content/articlelanding/2022/sd/d1sd00036e},
volume = {1},
