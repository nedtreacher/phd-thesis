issn = {25740970},
journal = {ACS Applied Nano Materials},
keywords = {Langmuir adsorption,X-ray photoelectron spectroscopy,adsorption isotherm,contact angle,graphene,monolayer,self-assembly,surface modification},
month = {6},
number = {6},
pages = {2718--2726},
publisher = {American Chemical Society},
title = {{Noncovalent monolayer modification of graphene using pyrene and cyclodextrin receptors for chemical sensing}},
