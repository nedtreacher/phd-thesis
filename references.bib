issn = {09254005},
journal = {Sensors and Actuators, B: Chemical},
keywords = {Field-effect transistor,Honeybee (Apis mellifera),Odorant detection,Odorant-binding protein,Olfactory electronic biosensor,Reduced graphene oxide},
month = {3},
