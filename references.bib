publisher = {AIP Publishing},
title = {{Carbon nanotube field effect transistor aptasensors for estrogen detection in liquids}},
url = {/avs/jvb/article/33/6/06F904/102545/Carbon-nanotube-field-effect-transistor},
volume = {33},
year = {2015}
}
@article{Zheng2017,
abstract = {Carbon nanotube network thin film transistors with channel lengths of 10-40 $\mu$m are fabricated from a nanotube buckypaper with 99\% semiconducting carbon nanotubes by surfactant free solution routes. The carbon nanotubes are suspended in 1,2-dichlorobenzene by ultrasonication at concentrations of 7.9 $\mu$g/ml, 3.4 $\mu$g/ml and 2.6 $\mu$g/ml, resulting in carbon nanotube field effect transistors with tube densities ranging from 5 tube/$\mu$m2 up to 32 tube/$\mu$m2. The device percolation threshold is 11 tube/$\mu$m2 with optimum device performances of on/off current ratio 7200, mobility 0.55 cm2/V{\textperiodcentered}s and Vth = +1 V. Devices with tube density above the percolation threshold are typically metallic-like. By encapsulating with poly-4-vinylphenol cross-linked with poly-(melamine-co-formaldehyde), the hysteresis becomes independent of channel length and the device performance approaches the best devices at the percolation threshold.},
author = {Zheng, H. Y. and Plank, N. O.V.},
doi = {10.1504/IJNT.2017.082473},
issn = {14757435},
journal = {International Journal of Nanotechnology},
keywords = {Carbon nanotubes,Encapsulation,Hysteresis,PVP,Poly-4-vinylphenol,Solution processing techniques,Thin film field effect transistor},
number = {1-6},
pages = {505--518},
publisher = {Inderscience Publishers},
title = {{Facile fabrication of carbon nanotube network thin film transistors for device platforms}},
volume = {14},
year = {2017}
