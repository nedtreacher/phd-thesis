doi = {10.1016/J.CARBON.2014.07.078},
file = {:C\:/Users/perkinne/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Feng et al. - 2014 - Back-gate graphene field-effect transistors with double conductance minima(2).pdf:pdf},
