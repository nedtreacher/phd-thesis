@article{Artyukhin2006,
abstract = {Carbon nanotube transistors are a promising platform for the next generation of nonoptical biosensors. However, the exact nature of the biomolecule interactions with nanotubes in these devices remains unknown, creating one of the major obstacles to their practical use. We assembled alternating layers of oppositely charged polyelectrolytes on the carbon nanotube transistors to mimic gating of these devices by charged molecules. The devices showed reproducible oscillations of the transistor threshold voltage depending on the polarity of the outer polymer layer in the multilayer film. This behavior shows excellent agreement with the predictions of a simple electrostatic model. Finally, we demonstrate that complex interactions of adsorbed species with the device substrate and the surrounding electrolyte can produce significant and sometimes unexpected effects on the device characteristics. {\textcopyright} 2006 American Chemical Society.},
author = {Artyukhin, Alexander B. and Stadermann, Michael and Friddle, Raymond W. and Stroeve, Pieter and Bakajin, Olgica and Noy, Aleksandr},
doi = {10.1021/NL061343J},
issn = {15306984},
journal = {Nano Letters},
month = {9},
number = {9},
pages = {2080--2085},
pmid = {16968029},
publisher = {American Chemical Society},
title = {{Controlled electrostatic gating of carbon nanotube FET devices}},
url = {https://pubs.acs.org/doi/full/10.1021/nl061343j},
volume = {6},
year = {2006}
}
@article{Shin2010,
abstract = {Contact angle goniometry is conducted for epitaxial graphene on SiC. Although only a single layer of epitaxial graphene exists on SiC, the contact angle drastically changes from 69° on SiC substrates to 92° on graphene. It is found that there is no thickness dependence of the contact angle from the measurements of single-, bi-, and multilayer graphene and highly ordered pyrolytic graphite (HOPG). After graphene is treated with oxygen plasma, the level of damage is investigated by Raman spectroscopy and the correlation between the level of disorder and wettability is reported. By using a low-power oxygen plasma treatment, the wettability of graphene is improved without additional damage, which can solve the adhesion issues involved in the fabrication of graphene devices. {\textcopyright} 2010 American Chemical Society.},
