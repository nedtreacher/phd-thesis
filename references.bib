journal = {Journal of the American Chemical Society},
month = {3},
number = {9},
pages = {2448},
pmid = {17284037},
publisher = {NIH Public Access},
title = {{Non-Covalent Functionalization of Carbon Nanotubes by Fluorescein-Polyethylene Glycol: Supramolecular Conjugates with pH Dependent Absorbance and Fluorescence}},
