file = {:C\:/Users/perkinne/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Padovani et al. - 2017 - A microscopic mechanism of dielectric breakdown in SiO2 films An insight from multi-scale modeling.pdf:pdf},
