title = {{ Etch Behavior of ALD Al 2 O 3 on HfSiO and HfSiON Stacks in Acidic and Basic Etchants }},
url = {https://iopscience.iop.org/article/10.1149/1.3554729 https://iopscience.iop.org/article/10.1149/1.3554729/meta},
